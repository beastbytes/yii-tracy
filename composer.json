{
  "name": "beastbytes/yii-tracy",
  "description": "Configuration to integrate the Tracy Yii Framework Latte Template Renderer",
  "keywords": [
    "yii",
    "Tracy",
    "debugger"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1",
    "latte/latte": "dev-master",
    "psr/container": "2.0",
    "tracy/tracy": "^3.0-dev",
    "yiisoft/aliases": "^3.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\": "src/"
    }
  },
  "extra": {
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "bootstrap-web": "bootstrap-web.php"
    }
  }
}
