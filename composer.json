{
  "name": "beastbytes/yii-tracy",
  "description": "Integrate the Tracy debugger into the Yii Framework",
  "keywords": [
    "yii",
    "Tracy",
    "debugger"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1",
    "psr/container": "^2.0",
    "tracy/tracy": "^3.0-dev",
    "yiisoft/aliases": "^3.0",
    "yiisoft/factory": "^1.0",
    "yiisoft/view": "^12.0",
    "yiisoft/yii-debug": "dev-master",
    "yiisoft/yii-http": "^1.0",
    "yiisoft/translator": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^5.0",
    "yiisoft/di": "^1.0",
    "yiisoft/dummy-provider": "^1.0",
    "yiisoft/test-support": "^3.0",
    "yiisoft/translator-message-php": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\Tests\\": "tests/"
    }
  },
  "extra": {
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "di-providers-web": "di-providers-web.php",
      "di-web": "di-web.php",
      "events-web": "events-web.php",
      "params-web": "params-web.php"
    }
  }
}
